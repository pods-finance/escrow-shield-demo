#!/bin/sh
set -e

docker compose -f docker-compose.yml down -v

sleep 5

docker compose -f docker-compose.yml up -d zokrates

sleep 25

docker compose -f docker-compose.yml up -d timber

sleep 10

docker compose -f docker-compose.yml up -d zapp

docker compose -f docker-compose.yml logs -f -n 1000 zapp timber zokrates
